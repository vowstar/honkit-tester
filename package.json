{
  "name": "honkit-tester",
  "version": "1.7.1",
  "description": "Tester for honkit plugins",
  "main": "index.js",
  "scripts": {
    "lint": "jshint index.js spec/**.js lib/**.js",
    "pretest": "npm run lint",
    "test": "istanbul cover -x **/spec/** jasmine-node -- spec --verbose --captureExceptions",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "homepage": "https://github.com/vowstar/honkit-tester",
  "repository": {
    "type": "git",
    "url": "https://github.com/vowstar/honkit-tester.git"
  },
  "bugs": {
    "url": "https://github.com/vowstar/honkit-tester/issues"
  },
  "keywords": [
    "honkit",
    "plugin",
    "tester"
  ],
  "author": "Huang Rui <vowstar@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "fs-extra": "^9.0.1",
    "gitbook-plugin-emphasize": "^1.1.0",
    "honkit": "^3.6.8",
    "lodash": "^4.17.20",
    "mkdirp": "^1.0.4",
    "npm": "^6.14.8",
    "q": "^1.5.1",
    "temp": "^0.9.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "jasmine-node": ">=3.0.0",
    "istanbul": ">=0.4.5",
    "jshint": ">=2.12.0",
    "coveralls": ">=3.1.0"
  }
}
